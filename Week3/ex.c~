include <stdio.h>
int main(void)
{
 int a[20], n, counter, temp;
 printf("enter n (<=20) \n");
 scanf("%d",&n);
 if(n>20)
 {
  printf("wrong input \n");
 }
 else
 {
 counter = 0;
 while (counter <n)
 { 
  printf("enter the next element \n");
  scanf("%d",&a[counter]);
  counter++ ;
 }
 temp=a[n-1];
 counter = n-2 ;
 while (counter >= 0)
 {  
  a[counter+1]=a[counter];
  counter--;
 }
 a[0]=temp;
 counter = 0;
 while (counter<n)
 {
  printf("%d \n",a[counter]);
  counter = counter+1;
 }
}
}
